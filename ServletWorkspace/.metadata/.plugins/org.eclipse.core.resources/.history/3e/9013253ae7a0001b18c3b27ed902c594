package com.te.sprmvc.beans;

import java.io.Serializable;
import java.util.Date;

import javax.persistence.*;

import lombok.Data;

@Data
@Entity
@Table(name="employee")
public class EmployeeBean implements Serializable {
	
	@Id
	@Column
	private Integer id;
	
	@Column
	private String name;
	
	@Column
	private Date date;
	
	@Column
	private String password;

}
