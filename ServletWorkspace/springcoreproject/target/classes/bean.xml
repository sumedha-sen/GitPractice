<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    
    <bean class="com.tyss.bean.Employee" id="emp" scope="prototype" />
    <bean class="com.tyss.bean.Employee" id="employee">

     <constructor-arg name="name" value="ABC" />
     <constructor-arg name="id" value="10" />
     </bean>
      <bean class="com.tyss.bean.Employee" id="employeeObject">

     <property name="name" value="sara" ></property>
     <property name="id" value="10" ></property>
     </bean>
     
     
    
    
   
</beans>